<div class="header">
    <div class="title shadow-normal"><img src="/images/home-house.png" height="14" /> &gt;
        @Model.GetCultureText("Print") &gt; @Model.GetCultureText("Settings")
        <div class="screen-number-right">[@Model.CurrentPage]</div>
    </div>
</div>
<div class="body">
    <table border=0 cellpadding=0 cellspacing=0 width="100%">
        <colgroups><col width="25%"><col width="25%"><col width="25%"><col width="25%"></colgroups>
        <tr><td colspan=2><div class="info2 shadow-normal">@Model.GetCultureText("FanSpeed")<div class="label-suffix">%</div></div></td><td colspan=2><div class="info2 shadow-normal">@Model.GetCultureText("PrintSpeed")<div class="label-suffix">%</div></div></td></tr>
        <tr><td colspan=2><div class="info2 shadow-normal">@Model.GetCultureText("NozzleTemp")<div class="label-suffix">°C</div></div></td><td colspan=2><div class="info2 shadow-normal">@Model.GetCultureText("BedTemp")<div class="label-suffix">°C</div></div></td></tr>
        <tr><td colspan=4><div class="info2 shadow-normal">@Model.GetCultureText("ZOffset")<div class="spinner-left spinner-left-@Model.CssClass">&nbsp;</div><div class="spinner-right spinner-right-@Model.CssClass">&nbsp;</div></div></td></tr>
    </table>
    <div class="buttons">
        <div class="button @Model.CssClass shadow-normal">@Model.GetCultureText("Back")</div>
    </div>
</div>