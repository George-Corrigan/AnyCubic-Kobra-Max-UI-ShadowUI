<div class="header">
    <div class="title shadow-normal"><img src="/images/home-house.png" height="14" /> &gt; @Model.GetCultureText("UserInput")
        <div class="screen-number-right">[@Model.CurrentPage]</div>
    </div>
</div>
<div class="body">
    <div style="height: 28px;"></div>
    <table border=0 cellpadding=0 cellspacing=0 width="100%">
        <colgroups><col width="33%"><col width="34%"><col width="33%"></colgroups>
        <tr><td colspan=3><div class="info shadow-normal" style="height: 37px;">&nbsp;</div></td></tr>
        <tr>
            <td><div class="button @Model.CssClass shadow-normal">1</div></td>
            <td><div class="button @Model.CssClass shadow-normal">2</div></td>
            <td><div class="button @Model.CssClass shadow-normal">3</div></td>
        </tr>
        <tr>
            <td><div class="button @Model.CssClass shadow-normal">4</div></td>
            <td><div class="button @Model.CssClass shadow-normal">5</div></td>
            <td><div class="button @Model.CssClass shadow-normal">6</div></td>
        </tr>
        <tr>
            <td><div class="button @Model.CssClass shadow-normal">7</div></td>
            <td><div class="button @Model.CssClass shadow-normal">8</div></td>
            <td><div class="button @Model.CssClass shadow-normal">9</div></td>
        </tr>
        <tr>
            <td><div class="button input-delete input-delete-@Model.CssClass shadow-normal">&nbsp;</div></td>
            <td><div class="button @Model.CssClass shadow-normal">0</div></td>
            <td><div class="button input-check input-check-@Model.CssClass shadow-normal">&nbsp;</div></td>
        </tr>

    </table>
    <div class="buttons">
        <div class="button @Model.CssClass shadow-normal">@Model.GetCultureText("Cancel")</div>
    </div>
</div>