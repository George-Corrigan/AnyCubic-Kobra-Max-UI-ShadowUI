<div class="header">
    <div class="title shadow-normal"><img src="/images/home-house.png" height="14" /> &gt;
        @Model.GetCultureText("System") &gt; @Model.GetCultureText("About")
        <div class="screen-number-right">[@Model.CurrentPage]</div>
    </div>
</div>
<div class="body">
    <table border=0 cellpadding=0 cellspacing=0 width="100%">
        <tr><td><div class="info2 shadow-normal" style="height: 49px;">@Model.GetCultureText("Machine")</div></td></tr>
        <tr><td><div class="info2 shadow-normal" style="height: 49px;">@Model.GetCultureText("FirmwareVersion")</div></td></tr>
        <tr><td><div class="info2 shadow-normal" style="height: 49px;">@Model.GetCultureText("UIVersion")<div class="version-info">@Model.GetCultureText("UIVersionNumber")</div></div></td></tr>
        <tr><td><div class="info2 shadow-normal" style="height: 49px;">@Model.GetCultureText("BuildVolume")</div></td></tr>
        <tr><td><div class="info2 shadow-normal" style="height: 49px;">@Model.GetCultureText("TechnicalSupport")</div></td></tr>
    </table>
    <div class="buttons">
        <div class="button @Model.CssClass shadow-normal">@Model.GetCultureText("Back")</div>
    </div>
</div>