<div class="header">
    <div class="title shadow-normal"><img src="/images/home-house.png" height="14" /> &gt;
        @Model.GetCultureText("Prepare") &gt; @Model.GetCultureText("Filament")
        <div class="screen-number-right">[@Model.CurrentPage]</div>
    </div>
</div>
<div class="body">
    <table border=0 cellpadding=0 cellspacing=0 width="100%">
        <tr><td><div class="info2 shadow-normal">@Model.GetCultureText("NozzleTemp")<div class="label-suffix" style="width: 87px;">°C</div></div></td></tr>
    </table>
    <div class="buttons">
        <div class="button @Model.CssClass shadow-normal">@Model.GetCultureText("FilamentIn")</div>
        <div class="button @Model.CssClass shadow-normal">@Model.GetCultureText("FilamentOut")</div>
        <div class="button @Model.CssClass shadow-normal">@Model.GetCultureText("Stop")</div>
        <div class="button @Model.CssClass shadow-normal">@Model.GetCultureText("Back")</div>
    </div>
</div>