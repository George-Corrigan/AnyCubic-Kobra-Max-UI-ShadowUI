<div class="header">
    <div class="title shadow-normal"><img src="/images/home-house.png" height="14" /> &gt;
        @Model.GetCultureText("System") &gt; @Model.GetCultureText("Speed")
        <div class="screen-number-right">[@Model.CurrentPage]</div>
    </div>
</div>
<div class="body">
    <table border=0 cellpadding=0 cellspacing=0 width="100%">
        <colgroups><col width="50%"><col width="50%"></colgroups>
        <tr><td colspan=4><div class="info2 shadow-normal">@Model.GetCultureText("CurrentFanSpeed")<div class="label-suffix">%</div></div></td></tr>
        <tr><td colspan=4><div class="info2 shadow-normal">@Model.GetCultureText("TargetFanSpeed")<div class="label-suffix">%</div><div class="spinner-left spinner-left-@Model.CssClass">&nbsp;</div><div class="spinner-right spinner-right-@Model.CssClass">&nbsp;</div></div></td></tr>
        <tr><td colspan=4><div class="info2 shadow-normal">@Model.GetCultureText("CurrentPrintRate")<div class="label-suffix">%</div></div></td></tr>
        <tr><td colspan=4><div class="info2 shadow-normal">@Model.GetCultureText("TargetPrintSpeed")<div class="label-suffix">%</div><div class="spinner-left spinner-left-@Model.CssClass">&nbsp;</div><div class="spinner-right spinner-right-@Model.CssClass">&nbsp;</div></div></td></tr>
    </table>
    <div class="buttons">
        <div class="button @Model.CssClass shadow-normal">@Model.GetCultureText("Set")</div>
        <div class="button @Model.CssClass shadow-normal">@Model.GetCultureText("Back")</div>
    </div>
</div>